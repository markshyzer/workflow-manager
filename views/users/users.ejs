<%- include('../partials/header.ejs') %>
<h1>All Users</h1>

<% users.forEach(function(u) { %>
    <div class="entry">
    <h4><%= u.name %></h4>
    Admin:
    <%= u.admin %>
    <%= u.email %>
    <%= u.address %>
    <%= u.city %>
    <%= u.prrovince %>
    <%= u.country %>
    <%= u.postalCode %>
    <%= u.timeZone %>
    <%= u.rate %>
    <%= u.hstNumber %>
    <%= u.googleId %>
    <a href="/users/<%= u._id %>">Detail</a>
    </div>  
<% }) %>